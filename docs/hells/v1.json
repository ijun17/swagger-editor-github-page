  "definitions": {
    "Ticket": {
      "required": [
        "expiredDatetime",
        "usableCount"
      ],
      "type": "object",
      "properties": {
        "expiredDatetime": {
          "title": "Expireddatetime",
          "type": "string",
          "format": "date-time"
        },
        "usableCount": {
          "title": "Usablecount",
          "type": "integer"
        }
      },
      "x-nullable": true
    },
    "User": {
      "type": "object",
      "properties": {
        "userId": {
          "title": "Userid",
          "type": "integer",
          "readOnly": true
        },
        "nickname": {
          "title": "Nickname",
          "type": "string",
          "readOnly": true,
          "minLength": 1
        },
        "role": {
          "title": "Role",
          "type": "string",
          "readOnly": true,
          "minLength": 1
        },
        "name": {
          "title": "Name",
          "type": "string",
          "readOnly": true,
          "minLength": 1
        },
        "phoneNumber": {
          "title": "Phonenumber",
