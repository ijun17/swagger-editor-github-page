  /images/:
    post:
      operationId: images_create
      summary: 이미지 업로드(관리자만 가능)
      description: 관리자가 이미지를 업로드합니다.
      parameters:
      - name: image
        in: formData
        required: true
        type: file
      responses:
        '201':
          description: 업로드 성공
          schema:
            $ref: '#/definitions/ImageResponse'
        '400':
          description: 잘못된 요청
        '401':
          description: 인증 필요
        '403':
          description: 권한 없음
      consumes:
      - multipart/form-data
      - application/x-www-form-urlencoded
      tags:
      - images
    parameters: []
  /images/{imageId}/:
    delete:
      operationId: images_delete
      summary: 이미지 삭제(관리자만 가능)
      description: 관리자가 업로드된 이미지를 삭제합니다.
      parameters: []
      responses:
        '204':
          description: 삭제 성공
        '401':
          description: 인증 필요
        '403':
          description: 권한 없음
        '404':
          description: 이미지를 찾을 수 없음
