  /shop/balances/:
    get:
      operationId: shop_balances_list
      description: |-
        잔액 관리 API
        - GET /api/shop/balances/my_balance/: 내 잔액 조회
        - POST /api/shop/balances/add_balance/: (관리자용) 잔액 추가
      parameters:
      - name: page
        in: query
