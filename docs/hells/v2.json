    parameters:
    - name: id
      in: path
      description: A unique integer value identifying this 예약.
      required: true
      type: integer
  /schedules/:
    get:
      operationId: schedules_list
      summary: 수업 일정 목록 조회
      description: 월별 수업 일정 목록을 조회합니다.
      parameters:
      - name: page
        in: query
        description: A page number within the paginated result set.
        required: false
        type: integer
      - name: month
        in: query
        description: '월 (예: 2023-05)'
